{
    "version": 2,
    "builds": [
      {
        "src": "/views/home.ejs**",
        "use": "@vercel/static"
      }
    ],
    "routes": [
        {
          "src": "/(.*)",
          "dest": "app.js",
          "external": true,
          "headers": {
            "host": "https://club-muniz.vercel.app/",
            "x-forwarded-port": "2000"
          }
        }
      ]
  }